		var friends = ["Raman", "Raj", "Aastha", "Arun"];
		document.write("<p> The first friend is <strong>");
		document.write(friends[0] + "</strong></p>");
		document.write("<p> The last friend is <strong>");
		document.write(friends[friends.length - 1] + "</strong></p>");
		friends.unshift("Deepti");
		document.write("<p> I all most forgot <strong>");
		document.write(friends[0]);
		document.write("</strong></p>");
	  </script>
